{
  "name": "philmareu/laramanager",
  "description": "Admin tool for Laravel applications",
  "keywords": ["laravel","admin"],
  "homepage": "https://github.com/philmareu/laramanager",
  "license": "MIT",
  "authors": [
    {
      "name": "Phil Mareu",
      "email": "philmareu@gmail.com",
      "homepage": "http://philmareu.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PhilMareu\\Laramanager\\": "src/PhilMareu/Laramanager"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.3|^8.0",
    "intervention/image": "^2.0",
    "intervention/imagecache": "^2.0",
    "illuminate/support": "^7.0|^8.0",
    "laravel/ui": "^2.0|^3.0",
    "suin/php-rss-writer": "^1.4",
    "doctrine/dbal": "^2.5"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "PhilMareu\\Laramanager\\Providers\\LaramanagerServiceProvider"
      ]
    }
  }
}
